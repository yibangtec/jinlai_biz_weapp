/* pages/item/index.wxss */
.outer {zoom:1;}    /*==for IE6/7 Maxthon2==*/
.outer::after {clear:both;content:'.';display:block;width: 0;height: 0;visibility:hidden;} 
page {  
  display: block; 
  width: 100%; 
  background-color: #f6f6f6;  
  position: relative;
}  
.temp-box{
  margin: 20rpx 20rpx;
  background-color: #fff; 
  border-radius: 20rpx;
  overflow: hidden;
  position: relative;
}
.tab{
  width: 25%;
  float: left;
  font-size: 26rpx;
  padding: 26rpx 0;
  text-align: center;
  color: #3E3A39;
}
.tab-current{
  border-bottom: 4rpx solid #FF3649;
}
.tips-text{
  font-size: 21rpx;
  text-align: center;
  color: #3E3A39;
  padding: 55rpx;
}
.freight{
  position: absolute;
  left: 50%;
  margin-left: -180rpx;
  width: 360rpx;
  height: 70rpx;
  font-size: 24rpx;
  line-height: 70rpx;
  border: 2rpx solid #FF3649;
  border-radius: 35rpx;
  color: #FF3649;
  text-align: center;
}
.tips-img{
  position: absolute;
  left: 50%;
  margin-left: -112rpx;
  width: 225rpx;
  height: 180rpx;
}
.create-box{
  float: left;
  width: 50%;
}
.create-item{
  width: 260rpx;
  height: 70rpx;
  font-size: 24rpx;
  line-height: 70rpx;
  border: 2rpx solid #FF3649;
  border-radius: 35rpx;
  color: #FF3649;
  text-align: center;
}
.item-list{
  display: flex;
  margin: 0 20rpx;
  border-bottom: 1rpx solid #c9caca;
}
.item-list-left{
  flex: 1;
}
.list-text{
  font-size: 26rpx;
  margin: 20rpx 0;
  color: #3E3A39;
}
.item-list-right{
  width: 180rpx;
}
.suk-list{
  height: 50rpx;
  padding-right: 18rpx;
  font-size: 24rpx;
  color: #FBA23D;
  border: 1rpx solid #FBA23D;
  border-radius: 25rpx;
  text-align: center;
  line-height: 45rpx;
  position: relative;
}
.list-img{
  width: 24rpx;
  height: 24rpx;
  position: absolute;
  top: 12rpx;
}
.select{
  width: 40rpx;
  height: 40rpx;
  margin: 20rpx 0 20rpx 20rpx;
  float: left
}
.select-list{
  float: left;
  width: 130rpx;
  height: 50rpx;
  font-size: 24rpx;
  border-radius: 25rpx;
  text-align: center;
  line-height: 45rpx;
  margin: 20rpx 0;
}
.select-left{
  color:#FF5065;
  border: 1rpx solid #FF5065;
  margin-right: 10rpx;
}
.select-right{
  color: #9FA0A0;
  border: 1rpx solid #9FA0A0;
  margin-right: 20rpx;
}

.bottom{
  width: 100%;
  position: fixed;
  bottom: 0;
}
.bottom-box{
  width: 100%;
  display: flex;
}
.bottomSelect{
  flex: 1;
  background-color:#fff;
  height: 85rpx;
}
.total{
  display: block;
  float: left;
  font-size: 26rpx;
  color: #3E3A39;
  padding: 10rpx;
  margin-top: 20rpx;
}
.bot-right{
  width: 480rpx;
}
.bottom-list{
  float: left;
  width: 160rpx;
  height: 85rpx;
  font-size: 26rpx;
  text-align: center;
  color: #fff;
  line-height: 80rpx;
}
.my-check{
  margin: 25rpx;
  float: left
}





.classname{
  border-bottom: 1rpx salmon solid;
  margin-bottom: 20rpx;
}
view{  
    box-sizing: border-box;  
}  
.item-box{  
    width: 700rpx;  
    margin: 0 auto;  
    padding:40rpx 0;  
}  
.items{  
    width: 100%;  
}  
.item{  
    position: relative;  
    border-top: 2rpx solid #eee;  
    height: 300rpx;  
    overflow: hidden;  
}  
.item:last-child{  
    border-bottom: 2rpx solid #eee;  
}  
.inner{  
    position: absolute;  
    top:0;  
}  
.inner.txt{  
    background-color: #fff;  
    width: 100%;  
    z-index: 50;  
    padding:0 10rpx;  
    transition: left 0.2s ease-in-out;  
    white-space:nowrap;  
    overflow:hidden;  
    text-overflow:ellipsis;  
}  
.inner.del{  
    background-color: #e64340;  
    width: 180rpx;text-align: center;  
    z-index: 2;  
    right: 0;  
    color: #fff  
}  
.item-icon{  
    width: 64rpx;  
    vertical-align: middle;  
    margin-right: 16rpx  
}  
.thumb{  
    width: 200px;  
    height: 200px;  
    -webkit-overflow-scrolling: touch;  
    overflow: scroll;  
}  


.show-btn {
  margin-top: 100rpx;
  color: #22cc22;
}
.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}
.modal-dialog {
  width: 540rpx;
  overflow: hidden;
  position: fixed;
  top: 30%;
  left: 0;
  z-index: 9999;
  background: #f9f9f9;
  margin: -180rpx 105rpx;
  border-radius: 36rpx;
}
.modal-title {
  padding-top: 50rpx;
  font-size: 36rpx;
  color: #030303;
  text-align: center;
}
.modal-content {
  padding: 50rpx 32rpx;
}
.modal-input {
  display: flex;
  background: #fff;
  border: 2rpx solid #ddd;
  border-radius: 4rpx;
  font-size: 28rpx;
}
.input {
  width: 100%;
  height: 82rpx;
  font-size: 28rpx;
  line-height: 28rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  color: #333;
}
input-holder {
  color: #666;
  font-size: 28rpx;
}
.modal-footer {
  display: flex;
  flex-direction: row;
  height: 86rpx;
  border-top: 1px solid #dedede;
  font-size: 34rpx;
  line-height: 86rpx;
}
.btn-cancel {
  width: 50%;
  color: #666;
  text-align: center;
  border-right: 1px solid #dedede;
}
.btn-confirm {
  width: 50%;
  color: #ec5300;
  text-align: center;
}
image{
  width: 200rpx;
  height: 200rpx;
}
